    "rxjs": "6.5.4",
    "typescript": "3.7.2"
  },
  "devDependencies": {
    "@types/node": "12.12.31",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "start-server-webpack-plugin": "2.2.5",
    "ts-loader": "6.2.2",
    "ts-node": "8.8.1",
    "webpack": "4.42.1",
