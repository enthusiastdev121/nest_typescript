    "@nestjs/schematics": "7.3.1",
    "@types/node": "14.17.4",
    "@typescript-eslint/eslint-plugin": "4.28.1",
    "@typescript-eslint/parser": "4.28.1",
    "eslint": "7.29.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.4",
    "start-server-webpack-plugin": "2.2.5",
